{
  "name": "iService",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "env:update": "bash ./scripts/update_env.sh",
    "lint": "eslint app/",
    "lint:fix": "eslint --fix app/",
    "prepush": "npm run lint && npm run test",
    "postinstall": "rm -rf .git/hooks/pre-push && node node_modules/husky/bin/install.js",
    "android": "ENVFILE=config/.env bash ./scripts/android/run.sh",
    "android:test": "ENVFILE=config/.env.test bash ./scripts/android/run.sh",
    "android:alpha": "ENVFILE=config/.env.alpha bash ./scripts/android/run.sh",
    "android:prod": "ENVFILE=config/.env.prod bash ./scripts/android/run.sh",
    "ios": "ENVFILE=config/.env bash ./scripts/ios/run.sh",
    "ios:test": "ENVFILE=config/.env.test bash ./scripts/ios/run.sh",
    "ios:alpha": "ENVFILE=config/.env.alpha bash ./scripts/ios/run.sh",
    "ios:prod": "ENVFILE=config/.env.prod bash ./scripts/ios/run.sh",
    "build:android:dev": "ENVFILE=config/.env bash ./scripts/android/start_build.sh",
    "build:android:test": "ENVFILE=config/.env.test bash ./scripts/android/start_build.sh",
    "build:android:alpha": "ENVFILE=config/.env.alpha bash ./scripts/android/start_build.sh",
    "build:android:prod": "ENVFILE=config/.env.prod bash ./scripts/android/start_build.sh",
    "build:ios:dev": "ENVFILE=config/.env bash ./scripts/ios/start_build.sh",
    "build:ios:test": "ENVFILE=config/.env.test bash ./scripts/ios/start_build.sh",
    "build:ios:alpha": "ENVFILE=config/.env.alpha bash ./scripts/ios/start_build.sh",
    "build:ios:prod": "ENVFILE=config/.env.prod bash ./scripts/ios/start_build.sh",
    "e2e-test:ios:dev": "BUILD_PLATFORM=IOS ENVFILE=config/.env bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:ios:test": "BUILD_PLATFORM=IOS ENVFILE=config/.env.test bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:ios:alpha": "BUILD_PLATFORM=IOS ENVFILE=config/.env.alpha bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:ios:prod": "BUILD_PLATFORM=IOS ENVFILE=config/.env.prod bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:android:dev": "BUILD_PLATFORM=ANDROID ENVFILE=config/.env bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:android:test": "BUILD_PLATFORM=ANDROID ENVFILE=config/.env.test bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:android:alpha": "BUILD_PLATFORM=ANDROID ENVFILE=config/.env.alpha bash ./scripts/saucelabs_uploader.sh",
    "e2e-test:android:prod": "BUILD_PLATFORM=ANDROID ENVFILE=config/.env.prod bash ./scripts/saucelabs_uploader.sh"
  },
  "rnpm": {
    "assets": [
      "resources/fonts"
    ]
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^2.7.1",
    "base-64": "^0.1.0",
    "exit-on-double-back": "^1.0.0",
    "geolib": "^2.0.23",
    "moment": "^2.18.1",
    "react": "16.0.0",
    "react-native": "0.50.3",
    "react-native-check-box": "^2.0.1",
    "react-native-circular-progress": "https://github.com/ayushinigam/react-native-circular-progress.git",
    "react-native-collapsible": "^0.9.0",
    "react-native-config": "^0.6.0",
    "react-native-device-info": "^0.11.0",
    "react-native-exception-handler": "^2.3.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-firebase": "^3.0.4",
    "react-native-google-analytics-bridge": "^5.3.3",
    "react-native-i18n": "^2.0.4",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-map-markerclustering": "^1.0.10",
    "react-native-maps": "https://github.com/dhansiddh/react-native-maps",
    "react-native-modal": "^3.1.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-restart": "^0.0.3",
    "react-native-simple-download-manager": "^1.2.0",
    "react-native-simple-toast": "^0.0.7",
    "react-native-splash-screen": "^3.0.0",
    "react-native-testfairy": "^2.1.6",
    "react-native-vector-icons": "^4.3.0",
    "react-native-version-number": "^0.1.3",
    "react-navigation": "^1.0.0-beta.21",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-ga-screen-tracker": "^1.3.0",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.15.6",
    "superagent": "^3.6.0",
    "xmlbuilder": "^9.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "^3.0.2",
    "codacy-coverage": "^2.0.2",
    "enzyme": "3.0.0-alpha.1",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-native": "^3.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.1.1",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.8.0",
    "react-addons-test-utils": "16.0.0-alpha.3",
    "react-dom": "16.0.0-beta.5",
    "react-native-debugger-open": "^0.3.12",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-saga-testing": "^1.0.5"
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-clone-referenced-element|react-navigation|exit-on-double-back)"
    ]
  }
}
